foreign:
  func emit(...) -> unit

func tests() -> unit:
  unsafe:
    -- upcast
    discard emit(123::int8 as real16)
    discard emit(123::int16 as real)
    discard emit(123::int as real64)

    -- downcast
    discard emit(123::real64 as int)
    discard emit(123::real as int16)
    discard emit(123::real16 as int8)
