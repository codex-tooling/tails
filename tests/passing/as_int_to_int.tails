foreign:
  func emit(...) -> unit

func tests() -> unit:
  unsafe:
    -- redundant cast
    discard emit(123::int as int)
    discard emit(123::nat as nat)

    -- int/nat conversion
    discard emit(123::int as nat)
    discard emit(123::nat as int)

    -- upcast
    discard emit(123::int8 as int16)
    discard emit(123::int16 as int)
    discard emit(123::int as int64)

    -- downcast
    discard emit(123::int64 as int)
    discard emit(123::int as int16)
    discard emit(123::int16 as int8)
